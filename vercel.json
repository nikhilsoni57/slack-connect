{
  "version": 2,
  "builds": [
    {
      "src": "nexecute-backend/package.json",
      "use": "@vercel/node",
      "config": {
        "outputDirectory": "nexecute-backend"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "nexecute-backend/dist/server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "nexecute-backend/dist/server.js": {
      "maxDuration": 30
    }
  }
}